#include <stdio.h>
#include <stdlib.h>
#define TAM 8

int main() {
    int vetor[TAM];
    int x, y;
    
    printf("Digite os valores do vetor:\n");
    for (int i = 0; i < TAM; i++) {
        scanf("%d", &vetor[i]);
    }
    
    printf("Digite o valor de X e Y (entre 0 e 7):\n");
    scanf("%d %d", &x, &y);
    
    printf("Soma dos valores de X e Y: %d\n", vetor[x] + vetor[y]);
    
    return 0;
}
