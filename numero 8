#include <stdio.h>
#include <stdlib.h>
#define TAM 6

int main() {
    int vetor[TAM];
    
    printf("Digite os valores:\n");
    for (int i = 0; i < TAM; i++) {
        scanf("%d", &vetor[i]);
    }
    
    printf("Valores na ordem inversa: ");
    for (int i = TAM - 1; i >= 0; i--) {
        printf("%d ", vetor[i]);
    }
    
    return 0;
}

